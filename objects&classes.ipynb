    "class ece:  #parent class\n",
    "    def __init__(self,name):\n",
    "        self.name=name\n",
    "        \n",
    "        \n",
    "class elect(ece):    #child class\n",
    "    def faculty(self):\n",
    "        print(\"sunny anand\")\n",
    "        \n",
    "class sp(ece):  #child class\n",
    "    def faculty(self):\n",
    "        print(\"rinki gupta\")\n",
    "        \n",
    "obv=[elect('harneet rana'),sp(\"supra dixit\")]   \n",
    "for i in obv:\n",
    "    print(i.name,\",\",end=\" \")\n",
    "    i.faculty()"
   ]
